<h1>JSON CRUD ( Create, Read , Update , Delete )</h1>
<a href="/add" class="btn-create">Create </a>
<table>
  <tr>
    <th>No</th>
    <th>Name</th>
    <th>Height</th>
    <th>Weight</th>
    <th>Birthdate</th>
    <th>Married</th>
    <th>Actions</th>
  </tr>
  <% data.forEach((item, index) => { %>
    <tr>
      <td><%= index + 1 %></td>
      <td><%= item.name %></td>
      <td><%= item.height %></td>
      <td><%= item.weight %></td>
      <td><%= item.birthdate %></td>
      <td><%= item.married ? 'Yes' : 'No' %></td>
      <td><a href="/update">Update</a> <a href="/delete">Delete</a> </td>
    </tr>
  <% }) %>
</table>
